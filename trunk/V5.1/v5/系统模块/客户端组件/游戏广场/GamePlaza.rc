IDR_COMMAND MENU 
BEGIN
    POPUP "游戏列表"
    BEGIN
        MENUITEM "进入游戏房间",                IDM_ENTER_SERVER
        MENUITEM "收缩列表",                    IDM_SHRINK_LIST
        MENUITEM "展开列表",                    IDM_EXPAND_LIST
    END
    POPUP "用户命令"
    BEGIN
        MENUITEM "用户登录",                    IDM_USER_LOGON
        MENUITEM "用户切换",                    IDM_SWITCH_ACCOUNTS
    END
    POPUP "游戏房间"
    BEGIN
        MENUITEM "更多颜色",                    IDM_MORE_COLOR
        MENUITEM SEPARATOR
        MENUITEM "游戏规则",                    IDM_GAME_RULE
        MENUITEM "游戏排行",                    IDM_GAME_CHART
        MENUITEM "关闭房间",                    IDM_DELETE_SERVER_ITEM
        MENUITEM SEPARATOR
        MENUITEM "显示所有信息",                IDM_MESSAGE_SHOW_ALL
        MENUITEM "屏蔽厌恶信息",                IDM_MESSAGE_HIDE_DETEST
        MENUITEM "只显示好友信息",              IDM_MESSAGE_ONLY_FRIEND
    END
    POPUP "用户菜单"
    BEGIN
        MENUITEM "与用户私聊",                  IDM_CREATE_ISER_WISPER
        MENUITEM "拷贝用户名",                  IDM_COPY_USER_NICKNAME
        MENUITEM SEPARATOR
        MENUITEM "设置为好友",                  IDM_SET_FRIEND_USER
        MENUITEM "设置为厌恶",                  IDM_SET_DETEST_USER
        MENUITEM SEPARATOR
        MENUITEM "设置聊天对象",                IDM_SET_CHAT_USER
        MENUITEM "取消聊天对象",                IDM_CANCEL_CHAT_USER
        MENUITEM SEPARATOR
        MENUITEM "查找在线用户",                IDM_SEARCH_ONLINE_USER
        MENUITEM "查看用户位置",                IDM_SEE_USER_LOCATION
        MENUITEM "查看用户资料",                IDM_SHOW_USER_INFO
        MENUITEM "用户信息管理",                IDM_USER_INFO_MANAGER
        MENUITEM "设置备注信息",                IDM_SET_USER_NOTE
        MENUITEM SEPARATOR
        MENUITEM "一起玩游戏",                  IDM_PLAY_GAME_TOGETHER
        MENUITEM "邀请加入游戏",                IDM_INVITE_USER
        MENUITEM "旁观用户游戏",                IDM_LOOKON_USER
        MENUITEM SEPARATOR
    END
    POPUP "私聊控制"
    BEGIN
        MENUITEM "按 Enter 键发送消息",         IDM_SEND_BY_ENTER
        MENUITEM "按 Ctrl+Enter 键发送消息",    IDM_SEND_BY_CTRL_ENTER
    END
    POPUP "管理控制"
    BEGIN
        POPUP "游戏玩家管理"
        BEGIN
            MENUITEM "踢用户下线",                  IDM_KILL_USER
            MENUITEM "禁止该用户大厅聊天",          32892
            MENUITEM "允许该用户大厅聊天",          32894
            MENUITEM "禁止该用户游戏聊天",          32896
            MENUITEM "允许该用户游戏聊天",          32902
            MENUITEM "禁止该用户私聊",              32899
            MENUITEM "允许该用户私聊",              32901
        END
        MENUITEM "游戏房间管理",                IDM_MANAGER_SERVER
        MENUITEM "系统消息管理",                IDM_MANAGER_MESSAGE
        MENUITEM SEPARATOR
        MENUITEM "踢用户下线...",               IDM_KILL_USER
    END
    POPUP "辅助命令"
    BEGIN
        MENUITEM "无效命令",                    IDM_NULL_COMMAND
    END
END

